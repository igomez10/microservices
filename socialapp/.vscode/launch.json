    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch socialapp",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "cmd/main.go",
            "args": [
                "--port=8085",
                "--proxy=http://localhost:9091",
                "--urlServiceHost=http://localhost:8087",
            ],
            "cwd": ".",
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}
